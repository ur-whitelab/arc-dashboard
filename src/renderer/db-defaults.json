[
  {
    "_id": "p1",
    "type": "process",
    "name": "Simulation",
    "status": "disabled",
    "docker_id": null
  },
  {
    "_id": "p2",
    "type": "process",
    "name": "Analysis",
    "status": "ready",
    "docker_id": "danalysis"
  },
  {
    "_id": "p3",
    "type": "process",
    "name": "Projection",
    "status": "ready",
    "docker_id": null,
    "cmd": "ar-project",
    "args": {}
  },
  {
    "_id": "pcustom",
    "type": "process",
    "name": "Custom",
    "status": "ready",
    "docker_id": null,
    "cmd": "",
    "args": {
      "exe": {
        "flag": "",
        "description": "Executable",
        "default": "ls"
      },
      "arg": {
        "flag": "-",
        "description": "Arguments",
        "default": "l"
      }
    }
  },
  {
    "_id": "p5",
    "type": "process",
    "name": "Vision",
    "status": "ready",
    "docker_id": null,
    "cmd": "arcvision",
    "args": {
      "hostname": {
        "flag": "--hostname",
        "description": "Hostname",
        "default": "*"
      },
      "zmq-port": {
        "flag": "--zmq-port",
        "description": "ZMQ PUb port",
        "default": "5001"
      },
      "video-filename": {
        "flag": "--video-filename",
        "description": "Video or Webcam Path",
        "default": "0"
      },
      "server-port": {
        "flag": "--server-port",
        "description": "Port for server",
        "default": "10030"
      }
    }
  },
  {
    "_id": "cdocker",
    "type": "config",
    "create_options": {
      "AttachStdin": false,
      "AttachStdout": true,
      "AttachStderr": true,
      "Tty": true
    }
  },
  {
    "_id": "cnetwork",
    "ports": {
      "zmq": 5000,
      "vision-zmq": 5001,
      "proxy": 8080,
      "video": 10030
    }
  },
  {
    "_id": "danalysis",
    "type": "docker",
    "image": "jupyter/scipy-notebook",
    "cmd": ["sleep", "10"]
  },
  {
    "_id": "startup",
    "steps": ["p1", "p2", "p3", "p5"]
  }
]