[
  {
    "_id": "p1",
    "type": "process",
    "name": "Simulation",
    "status": "disabled",
    "docker_id": null
  },
  {
    "_id": "p2",
    "type": "process",
    "name": "Analysis",
    "status": "ready",
    "docker_id": "danalysis"
  },
  {
    "_id": "p3",
    "type": "process",
    "name": "Projection",
    "status": "ready",
    "docker_id": null,
    "cmd": ["ar-project"]
  },
  {
    "_id": "p4",
    "type": "process",
    "name": "Camera",
    "status": "disabled",
    "docker_id": null,
    "cmd": [
      "vlc",
      {
        "name": "video source",
        "description": "VLC specific arg to find webcam.",
        "default": "v4l2:///dev/video0"
      },
      "-I dummy",
      "--sout",
      "#transcode{vcodec=theo,vb=800,fps=30,scale=1,acodec=none}:standard{access=http,mux=ogg,dst=:10030/stream.ogg}"],
    "cmd2": ["vlc", "-I dummy", null, "--sout", ":sout=#transcode{vcodec=h264,acodec=mpga,ab=128,channels=2,samplerate=44100}:http{mux=ffmpeg{mux=flv},dst=:8080/stream} :sout-keep"]
  },
  {
    "_id": "pcustom",
    "type": "process",
    "name": "Custom",
    "status": "ready",
    "docker_id": null,
    "cmd": [
      {
        "name": "exe",
        "description": "Executable",
        "default": "ls"
      },
      {
        "name": "arg",
        "description": "Arguments",
        "default": "-l"
      }
    ]
  },
  {
    "_id": "p5",
    "type": "process",
    "name": "Vision",
    "status": "ready",
    "docker_id": null,
    "cmd": [
      "arcvision",
      "--hostname", "*",
      "--zmq-port", "5001",
      "--server-port", "10030",
      "--video-filename",
      {
        "name": "video-filename",
        "description": "path to video file",
        "default": "0"
      }
    ]
  },
  {
    "_id": "cdocker",
    "type": "config",
    "create_options": {
      "AttachStdin": false,
      "AttachStdout": true,
      "AttachStderr": true,
      "Tty": true
    }
  },
  {
    "_id": "cnetwork",
    "ports": {
      "zmq": 5000,
      "vision-zmq": 5001,
      "proxy": 8080,
      "video": 10030
    }
  },
  {
    "_id": "danalysis",
    "type": "docker",
    "image": "jupyter/scipy-notebook",
    "cmd": ["sleep", "10"]
  },
  {
    "_id": "startup",
    "steps": ["p1", "p2", "p3", "p5"]
  }
]